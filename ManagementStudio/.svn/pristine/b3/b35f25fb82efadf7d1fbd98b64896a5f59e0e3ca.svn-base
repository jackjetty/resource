@IMPORT url("public.css");

body{background:#DFE8F6;overflow:hidden;}
.north{height:70px;overflow:hidden !important;border:none !important;background:#DFE8F6 !important;}
.south{height:26px;line-height:26px;*height:27px;*line-height:27px;overflow:hidden;border:none !important;background:url(../images/main/south-bg.gif) repeat-x 0 0 !important;text-align:center;}
.west{width:200px;border-bottom:none !important;}
.east{width:5px;background:#DFE8F6 !important;}
.south .version{float:left;margin-left:5px;}
.logo{width:482px;height:70px;}
.license{position:absolute;z-index:100;top:28px;left:500px;color:#1f336b;font-size:14px;}
.license a{font-size:14px;outline:none;color:red;}
.productType{position:absolute;left:480px;top:7px;font-size:14px;padding:2px;}
#load-mask{z-index:20000;position:absolute;left:0px;top:0px;width:100%;height:100%;background:white;opacity:1;filter:alpha(opacity=100);}
#loading{width:124px;*width:130px;margin-left:-62px;*margin-left:-65px;left:50%;height:34px;*height:40px;margin-top:-17px;*margin-top:-20px;top:50%;}
.loadmask{z-index:100;position:absolute;top:0;left:0;-moz-opacity:0.5;opacity:.50;filter:alpha(opacity=50);background-color:#fff;width:100%;height:100%;zoom:1;}
.loadmask-msg{z-index:20001;position:absolute;width:124px;height:34px;*width:130px;*height:40px;top:0;left:0;border:1px solid #6593cf;background:#c3daf9;padding:2px;}
.loadmask-msg div{padding:8px 15px 8px 15px;background:white url(../images/main/block-bg.gif) repeat-x;height:16px;line-height:16px;border:1px solid #a3bad9;color:#003366;font:normal 12px tahoma,arial,helvetica,sans-serif;cursor:wait;}
.masked{overflow:hidden !important;}
.masked-relative{position:relative !important;}
.masked-hidden{visibility:hidden !important;}
.navigator{position:absolute;width:177px;height:51px;float:right;right:7px;top:11px;}
.navigator ul{height:51px;margin:0;padding:0;list-style:none;}
.navigator ul li{width:33px;height:51px;padding-top:33px;text-align:center;float:left;cursor:pointer;}
.navigator ul li span{height:18px;line-height:18px;color:#000;}
.navigator ul li.home{background:url(../images/main/home.gif) no-repeat 0 0;}
.navigator ul li.password{background:url(../images/main/password.gif) no-repeat 0 0;margin-left:15px;}
.navigator ul li.refresh{background:url(../images/main/refresh.gif) no-repeat 0 0;margin-left:15px;}
.navigator ul li.exit{background:url(../images/main/exit.gif) no-repeat 0 0;margin-left:15px;}
.navigator ul li.hover{background-position:-33px 0;}
.navigator ul li.hover span{color:#f98a3a;}

.accordion{background:#fff;overflow:hidden;}
.accordion .accordion-header{background:#E0ECFF;border-top-width:0;cursor:pointer;border-right:none;border-left:none;}
.accordion .accordion-body{border-right:none;border-left:none;}
.accordion .accordion-header .panel-title{font-weight:normal;}
.accordion .accordion-header-selected .panel-title{font-weight:bold;}
.accordion-noborder .accordion-header{border-width:0 0 1px;}
.accordion-noborder .accordion-body{border-width:0px;}
.accordion-collapse{background-position:0px -285px !important;}
.accordion-expand{background-position:0px -270px !important;}

.layout{position:relative;overflow:hidden;margin:0;padding:0;}
.layout-panel{position:absolute;overflow:hidden;}
.layout-panel-east,.layout-panel-west{z-index:2;background1:#fff;}
.layout-panel-north,.layout-panel-south{z-index:3;background1:#fff;}
.layout-button-up{background:url('images/layout_button_up.gif') no-repeat;}
.layout-button-down{background:url('images/layout_button_down.gif') no-repeat;}
.layout-button-left{background-position:0px -180px !important;}
.layout-button-right{background-position:0px -165px !important;}
.layout-expand{position:absolute;padding:0px 5px;padding:0px;background:#D2E0F2;font-size:1px;cursor:pointer;z-index:1;}
.layout-expand .panel-header{background:transparent;border-bottom-width:0px;}
.layout-expand .panel-header .panel-tool{top:5px;}
.layout-expand .panel-body{overflow:hidden;}
.layout-expand-over{background:#D9E8FB;}
.layout-body{overflow:auto;background:#fff;}
.layout-split-proxy-h{position:absolute;width:5px;background:#ccc;font-size:1px;cursor:e-resize;display:none;z-index:5;}
.layout-split-proxy-v{position:absolute;height:5px;background:#ccc;font-size:1px;cursor:n-resize;display:none;z-index:5;}
.layout-split-north{border-bottom:5px solid #DFE8F6;}
.layout-split-south{border-top:5px solid #DFE8F6;}
.layout-split-east{border-left:5px solid #DFE8F6;}
.layout-split-west{border-right:5px solid #DFE8F6;}
.layout-mask{position:absolute;background:#fafafa;filter:alpha(opacity=10);opacity:0.10;z-index:4;}

.menu{position:absolute;background:#f0f0f0 url(../images/main/menu.gif) repeat-y;margin:0;padding:2px;border:1px solid #718bb7;overflow:hidden;}
.menu-item{position:relative;margin:0;padding:0;height:22px;line-height:20px;overflow:hidden;font-size:12px;cursor:pointer;border:1px solid transparent;_border:1px solid #f0f0f0;}
.menu-text{position:absolute;left:28px;top:0px;}
.menu-icon{position:absolute;width:16px;height:16px;top:3px;left:2px;}
.menu-rightarrow{position: absolute;width:4px;height:7px;top:7px;right:5px;background:url(../images/main/menu_rightarrow.gif) no-repeat;}
.menu-sep{margin:3px 0px 3px 24px;line-height:2px;display:block;font-size:1px;background-color:#e0e0e0;border-bottom:1px solid #fff;overflow:hidden;}
.menu-active{-moz-border-radius:3px;-webkit-border-radius:3px;background: #ebf3fd url(../images/main/item-over.gif) repeat-x left bottom;border:1px solid #aaccf6;padding: 0;}
.menu-shadow{position:absolute;background:#ddd;-moz-border-radius:5px;-webkit-border-radius:5px;-moz-box-shadow:2px 2px 3px rgba(0, 0, 0, 0.2);-webkit-box-shadow:2px 2px 3px rgba(0, 0, 0, 0.2);filter:progid:DXImageTransform.Microsoft.Blur(pixelRadius=2,MakeShadow=false,ShadowOpacity=0.2);}
.menu-item-disabled{opacity:0.5;filter:alpha(opacity=50);cursor:default;}
.menu-active-disabled{border-color:#d3d3d3;}

.panel{overflow:hidden;font-size:12px;}
.panel-header{padding:6px;line-height:15px;color:#15428b;font-weight:bold;font-size:12px;background:url(../images/main/panel-title.gif) repeat-x;position:relative;border:1px solid #99BBE8;}
.panel-title{background:url(../images/main/blank.gif) no-repeat;overflow:hidden;white-space:nowrap;*padding-top:2px;}
.panel-header-noborder{border-width:0px;border-bottom:1px solid #99BBE8;}
.panel-body{overflow:auto;border:1px solid #99BBE8;border-top-width:0px;}
.panel-body-noheader{border-top-width:1px;}
.panel-body-noborder{border-width:0px;}
.panel-with-icon{padding-left:21px;}
.panel-icon{position:absolute;left:5px;top:6px;width:16px;height:16px;}
.panel-tool{position:absolute;right:5px;top:6px;}
.panel-tool div{display:block;float:right;width:15px;height:15px;margin-left:2px;cursor:pointer;opacity:0.6;filter:alpha(opacity=60);background:url(../images/main/tool-sprites.gif) no-repeat 0px 0px;}
.panel-tool div.panel-tool-over{opacity:1;filter:alpha(opacity=100);}
.panel-tool-close{background-position:0px 0px;}
.panel-tool-min{background-position:0px -15px;}
.panel-tool-max{background-position:0px -30px;}
.panel-tool-restore{background-position:0px -45px;}
.panel-tool-collapse{background-position:0px -240px;}
.panel-tool-expand{background-position:0px -255px;}
.panel-loading{padding:11px 0px 10px 30px;background:url('images/panel_loading.gif') no-repeat 10px 10px;}

.tabs-container{overflow:hidden;background:#fff;}
.tabs-header{border:1px solid #8DB2E3;background:#E0ECFF;border-bottom:0px;position:relative;overflow:hidden;padding:0px;padding-top:2px;overflow:hidden;}
.tabs-header-noborder{border:0px;}
.tabs-header-plain{border:0px;background:transparent;}
.tabs-scroller-left{position:absolute;left:0px;top:2px;width:18px;height:26px!important;height:30px;font-size:1px;display:none;cursor:pointer;background:transparent url(../images/tab/scroll-left.gif) no-repeat -18px 0;}
.tabs-scroller-right{position:absolute;right:0;top:2px;width:18px;height:26px!important;height:30px;font-size:1px;display:none;cursor:pointer;background:transparent url(../images/tab/scroll-right.gif) no-repeat -18px 0px;}
.tabs-tool{position:absolute;top:-1px;border:1px solid #8DB2E3;padding:1px;background:#E0ECFF;overflow:hidden;}
.tabs-header-plain .tabs-scroller-left{top:2px;height:25px!important;height:27px;}
.tabs-header-plain .tabs-scroller-right{top:2px;height:25px!important;height:27px;}
.tabs-header-plain .tabs-tool{top:2px;padding-top:0;}
.tabs-scroller-over{background-position:0px 0px;}
.tabs-wrap{position:relative;left:0px;overflow:hidden;width:100%;margin:0px;padding:0px;}
.tabs-scrolling{margin-left:18px;margin-right:18px;}
.tabs{list-style-type:none;height:27px;margin:0px;padding:0 0 0 3px;*padding:0 0 0 8px;font-size:12px;width:5000px;background:url(../images/tab/tab-strip-bg.gif) repeat-x;}
.tabs li{float:left;display:inline-block;margin-top:1px;margin-right:3px;margin-bottom:0px;padding:0 0 0 2px;position:relative;background:url(../images/tab/tabs-sprite.gif) no-repeat top left;}
.tabs li a.tabs-inner{display:inline-block;text-decoration:none;color:#416AA3;margin:0px;padding:0px 35px 0px 10px;height:26px;line-height:23px;text-align:center;white-space:nowrap;background:url(../images/tab/tabs-sprite.gif) no-repeat top right;outline:none;}
.tabs li:hover{background:url(../images/tab/tabs-sprite-over.gif) no-repeat top left;}
.tabs li a.tabs-inner:hover{background:url(../images/tab/tabs-sprite-over.gif) no-repeat top right;}
.tabs li.tabs-selected{background:url(../images/tab/tabs-sprite-active.gif) no-repeat top left;}
.tabs li.tabs-selected a.tabs-inner{color:#416AA3;font-weight:bold;background:url(../images/tab/tabs-sprite-active.gif) no-repeat top right;outline:none;line-height:25px;}
.tabs li.tabs-selected a.tabs-inner:hover{cursor:default;background:url(../images/tab/tabs-sprite-active.gif) no-repeat top right;}
.tabs li.tabs-selected:hover{background:url(../images/tab/tabs-sprite-active.gif) no-repeat top left;}
.tabs-with-icon{padding-left:18px;}
.tabs-icon{position:absolute;width:16px;height:16px;left:10px;top:3px;}
.tabs li.tabs-selected .tabs-icon{top:4px;}
.tabs-closable{padding-right:8px;}
.tabs li a.tabs-close{position:absolute;font-size:1px;display:block;padding:0px;width:11px;height:11px;top:3px;right:4px;opacity:0.6;filter:alpha(opacity=60);background:url(../images/tab/tab-close.gif) no-repeat;outline:none;}
.tabs li a:hover.tabs-close{opacity:1;filter:alpha(opacity=100);cursor:hand;cursor:pointer;}
.tabs-panels{margin:0px;padding:0px;border:1px solid #8DB2E3;border-top:0px;overflow:hidden;}
.tabs-panels-noborder{border:0px;}


.tree{font-size:12px;margin:0;padding:0;list-style-type:none;}
.tree li{white-space:nowrap;}
.tree li ul{list-style-type:none;margin:0;padding:0;}
.tree-node{height:20px;white-space:nowrap;cursor:pointer;}
.tree-indent,.tree-icon{display:none !important;}
.tree-hit{cursor:pointer;}
.tree-expanded{display:inline-block;width:16px;height:18px;vertical-align:middle;background:url('images/tree_arrows.gif') no-repeat -18px 0px;}
.tree-expanded-hover{background:url('images/tree_arrows.gif') no-repeat -50px 0px;}
.tree-collapsed{display:inline-block;width:16px;height:18px;vertical-align:middle;background:url('images/tree_arrows.gif') no-repeat 0px 0px;}
.tree-collapsed-hover{background:url('images/tree_arrows.gif') no-repeat -32px 0px;}
.tree-folder{display:inline-block;background:url('images/tree_folder.gif') no-repeat;width:16px;height:20px;vertical-align:middle;}
.tree-folder-open{background:url('images/tree_folder_open.gif') no-repeat;}
.tree-file{display:inline-block;background-position:center center !important;width:16px;height:20px;vertical-align:middle;margin-left:37px;cursor:pointer;*margin-top:1px;}
.tree-loading{background:url('images/tree_loading.gif') no-repeat;}
.tree-title{display:inline-block;text-decoration:none;vertical-align:middle;white-space:nowrap;color:#15428B;width:100%;}
.tree-title a{outline:none;display:block;}
.tree-info{display:inline-block;text-decoration:none;vertical-align:middle;padding:0px 3px 0px 5px;white-space:nowrap;color:#15428B;cursor:pointer;*margin-top:2px;}
.tree-node-hover{background:#fafafa;}
.tree-node-selected{background:#FBEC88;}
.tree-checkbox{display:inline-block;width:16px;height:18px;vertical-align:middle;}
.tree-checkbox0{background:url('images/tree_checkbox_0.gif') no-repeat;}
.tree-checkbox1{background:url('images/tree_checkbox_1.gif') no-repeat;}
.tree-checkbox2{background:url('images/tree_checkbox_2.gif') no-repeat;}
.tree-node-proxy{font-size:12px;padding:1px 2px 1px 18px;background:#fafafa;border:1px solid #ccc;z-index:9900000;}
.tree-dnd-yes{background:url('images/tree_dnd_yes.png') no-repeat 0 center;}
.tree-dnd-no{background:url('images/tree_dnd_no.png') no-repeat 0 center;}
.tree-node-top{border-top:1px dotted red;}
.tree-node-bottom{border-bottom:1px dotted red;}
.tree-node-append .tree-title{border:1px dotted red;}
.tree-editor{border:1px solid #ccc;font-size:12px;line-height:16px;width:80px;position:absolute;top:0;}