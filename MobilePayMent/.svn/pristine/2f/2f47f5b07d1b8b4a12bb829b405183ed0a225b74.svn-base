package com.rising.mobilepayment.bean;

import java.util.Date;

//抽奖活动bean

public class SweepStakes {

	private Integer id;
	private Integer deductScore;
	private String eventName;
	private Date startTime;
	private Date endTime;
	private String status;
	private Integer times;
	private String type;
	private String lotteryImgBeforeName;
	private byte[] lotteryImgBefore;
	private String lotteryImgAfterName;
	private byte[] lotteryImgAfter;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getDeductScore() {
		return deductScore;
	}

	public void setDeductScore(Integer deductScore) {
		this.deductScore = deductScore;
	}

	public String getEventName() {
		return eventName;
	}

	public void setEventName(String eventName) {
		this.eventName = eventName;
	}

	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public Integer getTimes() {
		return times;
	}

	public void setTimes(Integer times) {
		this.times = times;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getLotteryImgBeforeName() {
		return lotteryImgBeforeName;
	}

	public void setLotteryImgBeforeName(String lotteryImgBeforeName) {
		this.lotteryImgBeforeName = lotteryImgBeforeName;
	}

	public byte[] getLotteryImgBefore() {
		return lotteryImgBefore;
	}

	public void setLotteryImgBefore(byte[] lotteryImgBefore) {
		this.lotteryImgBefore = lotteryImgBefore;
	}

	public String getLotteryImgAfterName() {
		return lotteryImgAfterName;
	}

	public void setLotteryImgAfterName(String lotteryImgAfterName) {
		this.lotteryImgAfterName = lotteryImgAfterName;
	}

	public byte[] getLotteryImgAfter() {
		return lotteryImgAfter;
	}

	public void setLotteryImgAfter(byte[] lotteryImgAfter) {
		this.lotteryImgAfter = lotteryImgAfter;
	}

}
