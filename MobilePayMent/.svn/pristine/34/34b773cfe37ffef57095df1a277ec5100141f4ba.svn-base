<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rising.mobilepayment.mapper.AppStartPictureMapper">
	<select id="find" resultType="com.rising.mobilepayment.bean.AppStartPicture">
		select * from RS_AppStartPicture where pictureName = #{param1} and operate = '启用'
	</select>
	<select id="getInfo" resultType="com.rising.mobilepayment.bean.AppStartPicture">
		select * from RS_AppStartPicture where operate = '启用'
	</select>	
</mapper>