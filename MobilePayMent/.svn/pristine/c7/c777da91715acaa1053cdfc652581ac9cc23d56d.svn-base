<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rising.general.mapper.BusinessMapper">
	<select id="findById" parameterType="java.lang.String" resultType="com.rising.general.bean.Business">
