<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rising.mobilepayment.mapper.ProductMessageTemplateMapper">
	<select id="getTemplateByIdAndType" parameterType="java.util.HashMap" resultType="com.rising.mobilepayment.bean.ProductMessageTemplate">
		select * from RS_ProductMessageTemplate where messageId = #{id} and messageType = #{type}
	</select>
</mapper>