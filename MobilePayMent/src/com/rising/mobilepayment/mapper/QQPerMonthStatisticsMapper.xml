<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rising.mobilepayment.mapper.QQPerMonthStatisticsMapper">
	<insert id="addStatisticsRecord" parameterType="com.rising.mobilepayment.bean.QQPerMonthStatistics">
		insert into RS_QQPerMonthStatistics (statisticsTime,statisticsType,kaiTongNumber,quXiaoNumber,kouFeiNumber)
		values (#{statisticsTime},#{statisticsType},#{kaiTongNumber},#{quXiaoNumber},#{kouFeiNumber}) 
	</insert>
</mapper>