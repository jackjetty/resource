<mapper namespace="com.rising.mobilepayment.mapper.UserCheckInMapper">
	
	<insert id="addCheckInRecord" parameterType="com.rising.mobilepayment.bean.UserCheckIn">
		insert into RS_UserCheckIn (phoneNumber,checkInTime,sendScore) values (#{phoneNumber},#{checkInTime},#{sendScore})
	</insert>
</mapper>