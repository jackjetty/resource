@charset "utf-8";
/* CSS Document */
*{
	margin:0;
	padding:0;
}
a{
	text-decoration:none;
}
body{
	background-color:#f2e7d5;
}
.page{
	
}
.page img{
	display:block;
	width:100%;
}
.page .main{
	margin:15px 15px 55px 15px;
	padding:15px;
	border-radius:10px;
	-o-border-radius:10px;
	-moz-border-radius:10px;
	-webkit-border-radius:10px;
	background-color:white;
	border:1px solid #c9c9c9;	
}
.page .main p{
	font-size:14px;
	font-family:"微软雅黑";
	color:#0d0d0d;
	text-indent:2em;
	line-height:28px;
}
.page .footer{
	padding:2px 0;
	overflow:hidden;
	background-color:#f8f8fa;
	border-top:1px solid #b6b6b6;
	width:100%;
	position:fixed;
	bottom:0px;
	_position:relative;
	_bottom: expression_r(documentElement.scrollTop + "px");
}
.page .footer .Left{
	float:left;
	width:50%;
}
.page .footer .Right{
	float:right;
	width:50%;
}
.page .footer span{
	width:100%;
	display:block;
	text-align:center;
	line-height:36px;
	font-size:14px;
	font-family:"微软雅黑";
	color:#6a6a6c;
	border-right:1px solid #b6b6b8;
		
}