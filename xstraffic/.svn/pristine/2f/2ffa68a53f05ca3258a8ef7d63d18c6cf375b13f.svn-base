.weixin.bgcolor {
  background: #E8E8E8;
}
.weixin .ui-content .ui-listview-inset,
.weixin .ui-panel-inner .ui-listview-inset {
  margin-top: 0.5em;
}
.weixin .ui-li-desc {
  margin: .5em 0 .6em;
  text-overflow: clip;
  font-size: 10pt;
  white-space: normal;
  line-height: 11pt;
}
.weixin .ui-li-static.ui-li {
  padding: 6px 20px;
}
.weixin .ga {
  height: 51px;
  text-align: right;
  padding: 18px 0px;
  width: 35%;
}
.weixin .gb {
  width: 63%;
}
.weixin h4 {
  margin: 4px 0px;
  font-size: 15px;
}
.weixin .localnav {
  margin: 0 0 20px 0;
}
.weixin .btn-block {
  display: block;
}
.weixin .scroll-paging {
  margin-top: 23px;
  font-weight: 800;
  text-align: center;
}
.weixin .scroll-paging .more {
  line-height: 23px;
}
.weixin .pager-main {
  padding-top: 0;
}
.weixin .default-border {
  border: 1px solid #bbb;
  border-radius: 3px;
}
.weixin .ui-content {
  padding-left: 6px;
  padding-right: 6px;
  padding-top: 2pt;
}
.weixin .search-map .ui-content {
  background: white;
  padding: 0;
}
.weixin .search-map iframe {
  border: none;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  position: absolute;
}
.weixin .fuid-row span {
  margin-right: 10px;
  display: inline-block;
  *zoom: 1;
}
.weixin .paging-loading {
  padding: 10px;
  text-align: center;
  margin-top: 30px;
}
.weixin .ui-btn-inner .ui-icon {
  right: 3px;
}
.weixin .search-form .ui-btn-up-c {
  font-weight: normal;
  font-size: 12px;
}
.weixin .search-form .ui-btn-inner {
  padding: 0;
  line-height: 34px;
  font-size: 12.5px;
}
.weixin .search-form .search-select .ui-btn-inner {
  text-align: left;
  padding-left: 3px;
}
.weixin .search-form .search-select .ui-select .ui-btn {
  padding: 0;
}
.weixin .search-form .search-filed {
  padding-left: 10px;
}
.weixin .search-form .search-btn {
  float: right;
}
.weixin .search-form .branch-search-content {
  padding: 15px;
}
.weixin .branch-search {
  padding-top: 2px;
}
.weixin .branch-search-content .tabs {
  width: 200px;
  margin-bottom: 20px;
}
.weixin .branch-search-content .normal-list {
  padding-top: 0;
}
.weixin .normal-list {
  padding-top: 13px;
}
.weixin .normal-list b {
  font-size: 18px;
}
.weixin .normal-list p.ui-li-desc {
  font-size: 16px;
  text-overflow: clip;
  line-height: 24px;
  white-space: normal;
}
.weixin .nopadding {
  padding-top: 0;
}
.weixin .paddingleft0 {
  padding-left: 0;
}
.weixin .nomargin {
  margin: 0;
}
.weixin .margin-bottom {
  margin-bottom: 7px;
}
.weixin .margin-top-10 {
  margin-top: 10px;
}
.weixin .normal-content {
  padding-top: 5px;
}
.weixin label.error {
  position: absolute;
  font-size: 9pt;
  color: #D30707;
  margin: 0;
  padding: 0;
  font-weight: normal;
}
.weixin .ui-btn-up-c span {
  font-weight: normal;
}
.weixin .content-primary .ui-listview .ui-li-divider {
  padding-left: 30px;
}
.weixin .content-primary .ui-listview .ui-btn-up-c {
  padding-left: 30px;
  padding-right: 30px;
}
.weixin .merchant-detail {
  padding-top: 0;
}
.weixin .merchant-activity .merchant-main {
  padding: 10px 10px;
  padding-bottom: 0;
}
.weixin .merchant-activity .merchant-main .left {
  float: left;
  width: 120px;
  margin-right: 10px;
}
.weixin .merchant-activity .merchant-main img {
  width: 120px;
  height: 120px;
  max-width: 95%;
}
.weixin .merchant-activity .merchant-main .merchant-title {
  vertical-align: top;
  padding-left: 10px;
}
.weixin .merchant-activity .btn {
  font-size: 12px;
  color: #2489ce;
  display: inline-block;
  *zoom: 1;
  float: right;
  margin: 0;
  padding: 6px 10px;
}
.weixin .merchant-activity .desc span {
  line-height: 32px;
  font-size: 14px;
}
.weixin .merchant-activity fieldset {
  border: 1px solid #ACACAC;
  margin: 0;
  margin-top: 10px;
  margin-bottom: 20px;
  marborder-radius: 3px;
}
.weixin .merchant-activity fieldset legend {
  color: #666;
}
.weixin .merchant-activity fieldset div.activity-info {
  padding: 0 8px;
  padding-top: 10px;
}
.weixin .merchant-activity fieldset div.desc {
  padding: 0 10px;
}
.weixin .merchant-activity fieldset .link-shops div.desc {
  border-bottom: 1px dotted #ccc;
}
.weixin .merchant-activity fieldset .link-shops div.desc:lst {
  border-bottom: none;
}
.weixin .merchant-activity h3 {
  margin: 0;
}
.weixin .merchant-activity .link-shops .more-shops {
  line-height: 32px;
  text-align: center;
}
.weixin .calculate-income div {
  margin: 1px auto;
  padding: 0;
  font-size: 16px;
}
.weixin .calculate-income p {
  margin: 1px auto;
  padding: 0;
  font-size: 16px;
}
.weixin .calculate-income Label {
  margin: 1px 1px 0px 0px;
  padding: 0;
  font-size: 16px;
}
.weixin .form-horizontal .field {
  text-align: right;
}
.weixin .form-horizontal .field label {
  line-height: 38pt;
  margin-right: 10px;
  font-size: 11pt;
}
.weixin .form-horizontal2 div.ui-input-text {
  margin-bottom: 0.8em;
}
.weixin .ui-listview .map-addr {
  position: absolute;
  right: 15px;
  bottom: 1.5pt;
}
.weixin .ui-listview .map-addr img {
  width: 32px;
  height: 32px;
}
.weixin .peccancy-inquiry .ui-select {
  margin: 0 0.3em;
}
.weixin .peccancy-inquiry .ui-select .ui-btn-text {
  font-size: 11pt;
}
.weixin .peccancy-inquiry .ui-select .ui-btn-icon-right .ui-btn-inner {
  padding-top: 0.4em;
  text-align: center;
  padding-left: 0;
  padding-right: 12px;
}
.weixin .peccancy-inquiry .ui-select .ui-btn-icon-right .ui-btn-inner .ui-icon {
  right: 0.3em;
  width: 1em;
  height: 1em;
}
.weixin .carnumselect label.error {
  left: -70%;
}
.html-pager {
  background: #f9f9f9;
  color: #333;
  font-family: Helvetica,STHeiti STXihei,Microsoft JhengHei,Microsoft YaHei,Tohoma,Arial;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  font-size: 12pt;
  padding: 15px 15px 0;
}
.html-pager .line {
  border-bottom: 1px solid #ddd;
  margin: 10px 0;
}
.html-pager .title {
  color: #000;
  font-size: 20px;
  font-weight: bold;
  word-break: normal;
  word-wrap: break-word;
}
.traffic .carNum {
  width: 120px;
  display: inline-block;
}
.traffic .in {
  display: inline-block;
  width: 200px;
  vertical-align: top;
}
.captcha div.ui-input-text {
  width: 40%;
  display: inline-block;
  margin-left: -5px;
  margin-top: 2px;
}
.captcha > .captcha-img {
  vertical-align: top;
  margin-top: 3px;
  width: 50%;
  height: 35px;
}
