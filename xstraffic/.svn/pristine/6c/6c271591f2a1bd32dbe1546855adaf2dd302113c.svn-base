@charset "utf-8";
/* CSS Document */
*{
	padding:0;
	margin:0;
}
a{
	text-decoration:none;
}
li{
	list-style:none;
}
body{
	background-color:#f5f5f9;
}
.page{
	overflow:hidden;
}
.page h1{
	padding:8px 10px 0 10px;
	background-color:white;
	width:100%;
	color:#333333;
	line-height:30px;
	font-size:14px;
	font-family:'Yuanti SC';
}
.page h2{
	padding:0 10px 8px 10px;
	background-color:white;
	width:100%;
	color:#333333;
	line-height:30px;
	font-size:14px;
	font-family:'Yuanti SC';
	border-bottom:1px solid #e5e5e5;
}
.page .Introduce{
	margin-top:8px;
	background-color:white;
	border-top:1px solid #e5e5e5;
	border-bottom:1px solid #e5e5e5;
}
.page .Introduce h3{
	padding:0 10px;
	color:#333333;
	line-height:44px;
	font-size:14px;
	font-family:'Yuanti SC';
	border-bottom:1px solid #e5e5e5;
}
.page .Introduce p{
	text-indent:2em;
	padding:10px;
	color:#757575;
	line-height:24px;
	font-size:14px;
	font-family:'Yuanti SC';
}
.page span{
	display:block;
	float:right;
	padding:8px 20px;
	color:#0077D8;
	line-height:44px;
	font-size:14px;
	font-family:'Yuanti SC';
	font-weight:bold;
}