@charset "utf-8";
/* CSS Document */
html, body {
	font-family: Microsoft YaHei, Helvitica, Verdana, Tohoma, Arial, san-serif;
	font-size: 12px;
	color: #666;
}
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, textarea, p, blockquote, th, td {
	margin: 0;
	padding: 0;
}
input{ -webkit-appearance: none;  }
input:focus{ outline-width:0;border-radius: 0;
    box-shadow: none !important;}
body{  background-color:#D5D5D6;  }
.list_h{background-color:#fff; margin:0 auto; padding-bottom:44px;box-shadow:0 1px 2px 0 rgba(0, 0, 0, 0.25); /*position: relative;*//* z-index:1;*/ min-height:480px; min-width:320px;max-width:800px;}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
fieldset, img {
	border: 0;
}
address, caption, cite, code, dfn, th, var {
	font-style: normal; 
	font-weight: normal;
}
ol, ul {
	list-style: none outside none;
	margin: 0;
	padding: 0;
}
caption, th {
	text-align: left;
}
h1, h2, h3, h4, h5, h6 {
	font-size: 100%;
	font-weight: normal;
}
a {
	text-decoration: none;
}
.clear {
	clear: both
}
.left {
	float: left
}
.right {
	float: right
}
.footer{ font-size:12px; margin:10px 0; text-align:center;}
.footer a{ color:#666;}
.red{ color:#FF0000}

